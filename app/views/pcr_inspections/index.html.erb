<p id="notice"><%= notice %></p>

<h1>Pcr Inspections</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Clinic</th>
      <th>Result</th>
      <th>Remarks</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pcr_inspections.each do |pcr_inspection| %>
      <tr>
        <td><%= pcr_inspection.subject_id %></td>
        <td><%= pcr_inspection.clinic_id %></td>
        <td><%= pcr_inspection.result %></td>
        <td><%= pcr_inspection.remarks %></td>
        <td><%= link_to 'Show', pcr_inspection %></td>
        <td><%= link_to 'Edit', edit_pcr_inspection_path(pcr_inspection) %></td>
        <td><%= link_to 'Destroy', pcr_inspection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pcr Inspection', new_pcr_inspection_path %>
